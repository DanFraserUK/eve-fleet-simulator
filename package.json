{
  "name": "eve_sim",
  "version": "1.0.0",
  "description": "simple sim to calculate fleet battle outcomes",
  "main": "lib/index.js",
  "scripts": {
    "build": "node build.js",
    "prepublish": "npm run build",
    "test": "npm run staticChecks",
    "staticChecks": "node node_modules/.bin/eslint src/*.js* src/**/*.js*; node node_modules/flow-coverage-report/bin/flow-coverage-report.js; node node_modules/.bin/flow; "
  },
  "flow-coverage-report": {
    "excludeGlob": [
      "./src/*/**.png",
      "./src/*JSON*",
      "./src/css/sim.worker.js"
    ],
    "includeGlob": [
      "./src/**/**.js*"
    ],
    "flowCommandPath": "./node_modules/flow-bin/cli.js",
    "type": [
      "html"
    ],
    "concurrentFiles": "8"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cordova": "^8.0.0",
    "css-loader": "^0.28.11",
    "electron": "^3.0.0",
    "electron-packager": "^12.1.2",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "flow-bin": "^0.61.0",
    "flow-coverage-report": "^0.4.1",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "worker-loader": "^2.0.0",
    "yarn": "^1.4.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-beautiful-dnd": "^7.1.3",
    "react-dom": "^16.2.0",
    "react-treebeard": "^2.1.0",
    "react-vis": "^1.9.1",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-less": "^2.3.1",
    "semantic-ui-react": "^0.78.2"
  }
}
