:root.defaultTheme {
  --color0: rgba(0, 0, 0, 0);
  --color1: rgb(0, 0, 0);
  --color2: rgb(27, 28, 29); /* menu topbar */
  --color3: rgb(83, 87, 123);
  --color4: rgb(170, 170, 230);
  --color5: rgb(180, 180, 180);
  --color6: rgb(33, 37, 43);
  --color6h: rgb(49, 54, 63);
  --color7: rgb(70, 70, 70);
  --color7t: rgba(70,70,70,0.85);
  --color8: rgb(118, 118, 118);
  --color9: rgb(255, 255, 255);
  --color10: rgb(249, 250, 251);
  --color11: rgb(232, 232, 232);
  --stock-table-border-color: rgba(34, 36, 38, 0.15);
  --stock-table-cell-border-color: rgba(34, 36, 38, 0.1);
  --input-text-color: rgba(0,0,0,.87);
  --input-text-color-focused: rgba(0,0,0,.8);
  --semantic-button-color-one:teal;
  --semantic-button-color-two:teal;
  --semantic-button-color-three:teal;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:olive;
  --semantic-invert-buttons:false;
  --stock-font-color: rgba(0, 0, 0, 0.87);
  --high-contrast-font-color: rgba(0, 0, 0, 0.95);
  --inverse-color: rgb(0, 0, 0);
  --button-opacity:1;
}
:root.shipwreckedTheme {
  --color0: rgba(0, 0, 0, 0.85);
  --color1: rgb(0, 0, 0);
  --color2: rgb(7, 28, 29);
  --color3: rgb(3, 87, 123);
  --color4: rgb(0, 170, 230);
  --color5: rgb(5, 180, 180);
  --color6: rgb(3, 37, 43);
  --color6h: rgb(9, 54, 63);
  --color7: rgb(0, 70, 70);
  --color7t: rgba(0,70,70,0.85);
  --color8: rgb(8, 118, 118);
  --color9: rgb(5, 55, 55);
  /* --color10: rgb(9, 0, 1); */
  --color10: var(--color6h);
  --color11: rgb(2, 32, 32);
  --stock-font-color: rgb(232,232,232);
  --high-contrast-font-color: rgb(249, 250, 251);
  --inverse-color: rgb(255, 255, 255);
  --stock-table-border-color: rgba(234, 236, 238, 0.35);
  --stock-table-cell-border-color: rgba(234, 236, 238, 0.2);
  --input-text-color: rgba(232,232,232,.87);
  --input-text-color-focused: rgba(232,232,232,0.8);
  --semantic-button-color-one:olive;
  --semantic-button-color-two:teal;
  --semantic-button-color-three:blue;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:vk;
  --button-opacity:1;
}
/* default firefox dark theme */
:root.darkTheme {
  --color2: rgba(37,44,51,1);
  --color9: rgba(17,19,21,1);
  --color6: rgba(24,29,32,1);
  --stock-font-color: rgba(143, 161, 178, 1);
  --high-contrast-font-color: rgba(169, 186, 203, 1);
  --inverse-color: rgb(255, 255, 255);
  --input-text-color: var(--stock-font-color);
  --input-text-color-focused: rgba(143,161,178,0.93);

  --color3: var(--color6);
  --color4: rgba(0,0,0,1);
  --color5: rgba(52,60,69,1);
  --color6h: var(--color5);
  --color6a: var(--color2);
  --color10: var(--color6);
  /*--color11: var(--color9);*/
  --color11: var(--color2);
  --color7: var(--color6);
  --color7t: rgba(24,29,32,0.85);

  /*--button-opacity:0.82;*/
  --button-opacity:1;
  --semantic-invert-buttons:false;
  --semantic-button-color-one:pale-blue;
  --semantic-button-color-two:pale-blue;
  --semantic-button-color-three:pale-blue;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:grey-blue;
}

/* default firefox light theme */
:root.lightTheme {
  --color2: rgba(235, 236, 237, 1);
  --color9: rgba(252, 252, 252, 1);
  --color6: rgba(247, 247, 247, 1);
  --stock-font-color: rgba(24, 25, 26, 1);
  --high-contrast-font-color: rgba(41, 46, 51, 1);
  --inverse-color: rgb(0, 0, 0);
  --input-text-color: var(--stock-font-color);
  --input-text-color-focused: rgba(24,25,26,0.93);

  --color3: var(--color6);
  --color4: rgba(170, 170, 170, 1);
  --color5: rgba(240, 241, 242, 1);
  --color6h: var(--color5);
  --color6a: var(--color2);
  --color10: var(--color6);
  --color11: var(--color2);
  --color7: var(--color6);
  --color7t: rgba(247,247,247,0.85);

  /*--button-opacity:0.9;*/
  --button-opacity:1;
  --semantic-invert-buttons:false;
  --semantic-button-color-one:mid-blue;
  --semantic-button-color-two:mid-blue;
  --semantic-button-color-three:mid-blue;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:bright-blue;
}
